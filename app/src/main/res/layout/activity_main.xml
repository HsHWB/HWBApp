<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.huehn.initword.activity.MainActivity"
    tools:viewBindingIgnore="false">
    <TextView
        android:id="@+id/goto_rxjava"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="RxJava"/>

    <TextView
        android:id="@+id/text"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Hello World!"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/goto_view"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="自定义view"/>

    <TextView
        android:id="@+id/goto_weex"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/text"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="跳转weex"/>

    <TextView
        android:id="@+id/goto_downfile"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_weex"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="下载文件"/>

    <TextView
        android:id="@+id/goto_facebook_login"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_weex"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="facebook登陆"/>

    <TextView
        android:id="@+id/goto_two_side_view"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_weex"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="两边滑动的seekbar控件"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/goto_corner_web_view"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_weex"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="顶部圆角底部直角的webview"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/goto_service_activity"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_weex"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="打开主进程的activity调起service"
        android:visibility="gone"/>


    <TextView
        android:id="@+id/goto_remote_activity"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_service_activity"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="打开另外进程(remote)的activity调起service"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/goto_drawelayout_activity"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_service_activity"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="侧滑菜单viewpager"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/goto_viewdrag_activity"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_service_activity"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="ViewDragHelper动画"/>

    <TextView
        android:id="@+id/goto_kotlin"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/goto_service_activity"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center"
        android:text="打开kotlin"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/goto_class_override"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="重写类"/>

    <ImageView
        android:id="@+id/imageview"
        android:layout_width="160dp"
        android:layout_height="90dp"
        android:layout_marginTop="50dp"/>
    <ImageView
        android:id="@+id/imageview2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"/>
</LinearLayout>
